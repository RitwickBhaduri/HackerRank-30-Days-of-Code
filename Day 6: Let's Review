#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        string str;
        cin>>str;
        for(int j=0;j<str.length();j+=2)
        {
            cout<<str[j];
        }
        cout<<" ";
        for(int j=1;j<str.length();j+=2)
        {
            cout<<str[j];
        }
        cout<<"\n";
    } 
    return 0;
}
